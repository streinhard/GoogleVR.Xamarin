<?xml version="1.0"?>
<package>
  <metadata>
    <id>GoogleVR.Widgets.Forms</id>
    <title>Google VR Widgets for Xamarin.Forms</title>
    <version>$version$</version>
    <authors>Stefan Reinhard</authors>
    <owners>Bitforge AG</owners>
    <summary>360 Video and Panorama widgets with support for Cardboard.</summary>
    <description>Google VR widgets (360 Videos and Panoramas) for Xamarin.Forms.</description>
    <iconUrl>https://vr.google.com/cardboard/images/hero-cardboard-download-mobile.jpg</iconUrl>
    <dependencies>
      <group targetFramework="MonoAndroid">
        <dependency id="GoogleVR.Widgets" version="$version$" />
        <dependency id="Xamarin.Forms" version="3.4.0.1008975" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="GoogleVR.Widgets" version="$version$" />
        <dependency id="Xamarin.Forms" version="3.4.0.1008975" />
      </group>
      <group targetFramework="netstandard2.0">
        <dependency id="Xamarin.Forms" version="3.4.0.1008975" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!-- .NET Standard 2.0 -->
    <file src="../src/GoogleVR.Forms/bin/Release/netstandard2.0/GoogleVR.Forms.dll" target="lib/netstandard2.0" />

    <!-- Android -->
    <file src="../src/GoogleVR.Forms.Droid/bin/Release/GoogleVR.Forms.Droid.dll" target="lib/MonoAndroid80" />

    <!-- iOS -->
    <file src="../src/GoogleVR.Forms.iOS/bin/Release/GoogleVR.Forms.iOS.dll" target="lib/Xamarin.iOS10" />
  </files>
</package>