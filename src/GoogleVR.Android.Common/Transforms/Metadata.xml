    <remove-node path="/api/package[@name='com.google.vr.cardboard']" />
    <remove-node path="/api/package[@name='com.google.vr.cardboard.annotations']" />
    <remove-node path="/api/package[@name='com.google.vr.ndk.base']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.base.api']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.common.api']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.controller.api']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.library.api']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.logging.api']" />
    <remove-node path="/api/package[@name='com.google.vr.vrcore.performance.api']" />
